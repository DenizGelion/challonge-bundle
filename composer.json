{
    "name": "denizgelion/challonge-bundle",
    "type": "contao-bundle",
    "description": "Challonge integration for Contao 4",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Deniz Gelion",
            "homepage": "https://github.com/DenizGelion"
        }
    ],
    "require": {
        "php": "^7.1",
        "contao/core-bundle": "^4.6"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
    },
    "extra": {
        "contao-manager-plugin": "Contao\\ChallongeBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Contao\\ChallongeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Contao\\ChallongeBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/DenizGelion/challonge-bundle/issues",
        "source": "https://github.com/DenizGelion/challonge-bundle"
    }
}
